Content-Type: application/json
Accept: application/json

{
  "description": "Dock Crane A",
  "operationalCapacity": 25.5,
  "setupTime": 10,
  "physicalResourceType": "Truck",
  "qualificationID": "3ead253f-c5aa-4d03-a290-36a65521fb3d"
}


